{
  "enabled": true,
  "version": "0.2",
  "language": "en",
  "useGitignore": true,
  "allowCompoundWords": true,
  "words": [
    "bumpp",
    "changelogithub",
    "destructurable",
    "echarts",
    "INTLIFY",
    "nocheck",
    "ntnyq",
    "pinia",
    "tsbuildinfo",
    "unocss",
    "unplugin",
    "valibot",
    "vite",
    "vueuse",
    "xgplayer"
  ],
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    "**/*.tsbuildinfo",
    "**/yarn.lock",
    "**/pnpm-lock.yaml",
    "**/package-lock.json",
    "*.spec.js.snap",
    "*.min.*",
    "*.log*",
    "dist"
  ]
}
