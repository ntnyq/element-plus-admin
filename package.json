{
  "name": "element-boilerplate",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:analyzer": "vue-cli-service build --report && anywhere -d dist -f /report.html",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "vue-cli-service lint",
    "lint:style": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "new": "plop",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": "vue-cli-service lint",
    ".{scss,vue}": "stylelint --fix"
  },
  "dependencies": {
    "@goy/utils": "^0.0.1",
    "axios": "^0.21.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "element-ui": "^2.13.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "qs": "^6.9.3",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vue-i18n": "^8.17.3",
    "vue-router": "^3.1.6",
    "vuex": "^3.2.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^0.1.0",
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "anywhere": "^1.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^7.13.0",
    "eslint-plugin-vue": "^7.1.0",
    "lint-staged": "^10.5.1",
    "node-sass": "^5.0.0",
    "plop": "^2.7.4",
    "sass-loader": "^10.0.5",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "svg-sprite-loader": "^5.0.0",
    "svgo": "^1.3.2",
    "vue-cli-plugin-element": "^1.0.0",
    "vue-template-compiler": "^2.6.12"
  },
  "repository": "https://github.com/ntnyq/element-boilerplate.git",
  "author": "ntnyq <ntnyq13@gmail.com> (https://ntnyq.com)",
  "license": "MIT"
}
