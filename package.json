{
  "name": "element-boilerplate",
  "version": "0.1.5",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:analyzer": "vue-cli-service build --report && anywhere -d dist -f /report.html",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "vue-cli-service lint",
    "lint:style": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "new": "plop",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ],
    "!(_variables).{scss,vue}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@goy/utils": "^0.0.1",
    "axios": "^0.19.0",
    "element-ui": "^2.12.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "qs": "^6.8.0",
    "vue": "^2.6.9",
    "vue-clipboard2": "^0.3.1",
    "vue-i18n": "^8.14.1",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "anywhere": "^1.5.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^6.4.0",
    "eslint-config-goy": "^0.0.5",
    "eslint-plugin-vue": "^5.2.3",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "lint-staged": "^9.2.5",
    "node-sass": "^4.12.0",
    "plop": "^2.4.0",
    "sass-loader": "^8.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.11.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.3.0",
    "vue-cli-plugin-element": "^1.0.0",
    "vue-template-compiler": "^2.6.9"
  },
  "repository": "https://github.com/ntnyq/element-boilerplate.git",
  "author": "ntnyq <ntnyq13@gmail.com> (https://ntnyq.com)",
  "license": "MIT"
}
